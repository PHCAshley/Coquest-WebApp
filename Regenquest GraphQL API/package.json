{
  "name": "sample-http",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "dev": "run-script-os",
    "dev:windows": "concurrently --raw \"npm run dev-build > nul\" \"npm run dev-start\"",
    "dev:nix": "concurrently --raw \"npm run dev-build > /dev/null\" \"npm run dev-start\"",
    "dev-build": "tsc --watch",
    "dev-start": "nodemon --watch dist/ --exec functions-framework --target=handler",
    "build": "tsc",
    "start": "functions-framework --target=handler",
    "lint": "run-script-os",
    "lint:win32": "eslint . & prettier . -c",
    "lint:darwin": "eslint .; prettier . -c",
    "lint:linux": "eslint .; prettier . -c",
    "lint-fix": "run-script-os",
    "lint-fix:win32": "eslint --fix . & prettier . --write",
    "lint-fix:darwin": "eslint --fix .; prettier . --write",
    "lint-fix:linux": "eslint --fix .; prettier . --write"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/storage": "^7.8.0",
    "apollo-server": "^3.13.0",
    "apollo-server-cloud-functions": "^3.13.0",
    "apollo-server-express": "^2.26.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "graphql": "^15.5.0",
    "graphql-tools": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.1",
    "path": "^0.12.7",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@google-cloud/functions-framework": "^3.3.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.5.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.4.0",
    "prettier": "^3.3.2",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
